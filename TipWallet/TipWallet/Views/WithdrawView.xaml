<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TipWallet.Views.WithdrawView">
    <ContentPage.Content>
        <StackLayout>
            <TableView>
                <TableRoot Title="Withdraw">
                    <TableSection>
                        <EntryCell Label="Amount" Text="{Binding WithdrawModel.Amount}"/>
                        <EntryCell Label="Description" Text="{Binding Withdraw.Description}"/>
                        <EntryCell Label="TimeStamp" Text="{Binding WithdrawModel.TimeStamp}"/>
                    </TableSection>
                </TableRoot>
            </TableView>
            <Button Text="Submit"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>